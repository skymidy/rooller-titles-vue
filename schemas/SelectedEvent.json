{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "title": "EventData",
  "type": "object",
  "additionalProperties": false,
  "properties": {
    "event_id": { "type": "string" },
    "event_sub_dists": {
      "type": "array",
      "items": {
        "type": "object",
        "additionalProperties": false,
        "properties": {
          "dist_tag": { "type": "string" }
        }
      }
    },
    "teams": {
      "type": "array",
      "items": { "type": "string" }
    },
    "dist": {
      "type": "array",
      "items": { "type": "string" }
    },
    "subs": {
      "type": "array",
      "items": { "$ref": "SelectedSubEvent.json" }
    },
    "sub_id": { "type": "string" },
    "geoname": { "type": "string" },
    "logo": { "type": ["string", "null"] },
    "unix_timestamp": { "type": "number" },
    "banner_top": { "type": ["string", "null"] },
    "banner_bottom": { "type": ["string", "null"] }
  }
}
